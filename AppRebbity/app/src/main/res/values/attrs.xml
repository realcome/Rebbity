<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VToolBar"> 
        <attr name="shadow" format="reference"/>
        <attr name="hasRainbowAnimBar" format="boolean"/>
    	<attr name="titleAlign">
	        <enum name="left" value="0" />
	        <enum name="middle" value="1" />
	        <enum name="right" value="2" />
    	</attr>
    	<attr name="toolbarIsTop" format="boolean"></attr>
    </declare-styleable>
    
    
    <declare-styleable name="CircleButton">
        <attr name="cb_color" format="color" />
        <attr name="cb_pressedRingWidth" format="dimension" />
        <attr name="cb_hasBgBeforePress" format="boolean"></attr>
    </declare-styleable>
</resources>
